        header.mui-appbar.mui--z1
            div.mui-container
                table(width="100%")
                    tr.mui--appbar-height
                        td.mui--text-title
                            a(href="/") rodb.kudesnik.cc
                        td(align="right")
                            ul.mui-list--inline.mui--text-body2
